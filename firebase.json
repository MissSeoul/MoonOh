{
  "hosting": {
    "public": "public",
    "headers": [
      {
        "source": "**/*.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/front.html"
      },
      {
        "source": "/favicon.ico",
        "destination": "/empty-file.ico"
      }
    ]
  }
}
